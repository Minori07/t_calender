<header>
	<div class="geer"">
		<%= link_to image_tag('geer.png'), members_cfg_path %>
	</div>
</header>
<div class=container>
	<div style="width: 300px;">
	  <%= form_for(@member) do |f| %>
	  <div>
	    <%= "名前" %>
	    <%= f.text_field :name, size: 20 %>
	  </div>
	  <div>
	    <%= f.label :date %>
	    <%= f.date_select :date %>
	  </div>
	    <%= f.submit 'Submit' %>
	  <% end %>
	</div>
	<p><%= link_to '一覧に戻る', members_url %></p>
</div>